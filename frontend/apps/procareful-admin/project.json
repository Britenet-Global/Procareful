{
  "name": "procareful-admin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/procareful-admin/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/procareful-admin"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "procareful-admin:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "procareful-admin:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "procareful-admin:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "procareful-admin:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "procareful-admin:build:development"
        },
        "production": {
          "buildTarget": "procareful-admin:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/apps/procareful-admin"
      },
      "configurations": {
        "ci": {
          "ci": true
        },
        "coverage": {
          "coverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["tsc -p tsconfig.app.json --noEmit"],
        "cwd": "apps/procareful-admin",
        "forwardAllArgs": false
      }
    }
  },
  "tags": ["admin"]
}

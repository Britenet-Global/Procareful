{
  "singleQuote": true,
  "printWidth": 100,
  "trailingComma": "es5",
  "tabWidth": 2,
  "arrowParens": "avoid",
  "semi": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^\\react(.*)",
    "^\\antd(.*)",
    "@/(.*)",
    "^\\@components/(.*)$",
    "^\\@assets/(.*)$",
    "^.*\\.svg$",
    "^[./]"
  ]
}
